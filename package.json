{
  "name": "dinos",
  "description": "δεινός",
  "version": "0.2.7",
  "main": "index.js",
  "repository": "https://github.com/0xflotus/dinos.git",
  "homepage": "https://yarnpkg.com/en/package/dinos",
  "author": "0xflotus <0xflotus@gmail.com>",
  "scripts": {
    "coc": "weallbehave -o .",
    "prelint": "prettier --check cli.js",
    "lint": "eslint cli.js",
    "postlint": "yarn jshint",
    "jshint": "jshint cli.js",
    "prelint:docs": "prettier --check docs/*.js",
    "lint:docs": "eslint docs/*.js"
  },
  "keywords": [
    "dns"
  ],
  "bin": {
    "dinos": "./index.js"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^5.0.0",
    "consoleplusplus": "^1.4.1",
    "cosmiconfig": "^6.0.0",
    "dayjs": "^1.8.23"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "jshint": "^2.11.0",
    "prettier": "^2.0.2",
    "weallbehave": "^1.2.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "globals": {
      "document": "readable"
    },
    "rules": {
      "indent": [
        "warn",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off",
      "dot-notation": [
        "error",
        {
          "allowKeywords": false
        }
      ],
      "curly": [
        "error"
      ],
      "func-names": [
        "error",
        "as-needed"
      ],
      "max-lines-per-function": [
        "error",
        {
          "max": 65,
          "skipBlankLines": true,
          "skipComments": true,
          "IIFEs": true
        }
      ],
      "newline-before-return": [
        "warn"
      ],
      "yoda": [
        "warn",
        "always"
      ],
      "use-isnan": "error",
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "array-bracket-spacing": [
        "warn",
        "never"
      ],
      "require-yield": "warn",
      "require-await": "warn",
      "dot-location": [
        "warn",
        "property"
      ],
      "comma-spacing": [
        "warn",
        {
          "before": false,
          "after": true
        }
      ],
      "no-unused-vars": "error"
    }
  },
  "jshintConfig": {
    "esversion": 8,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "futurehostile": true,
    "latedef": true,
    "leanswitch": true,
    "maxdepth": 5,
    "maxerr": 75,
    "maxparams": 4,
    "noarg": true,
    "nocomma": true,
    "nonew": true,
    "noreturnawait": true,
    "regexpu": true,
    "trailingcomma": false,
    "undef": true,
    "globals": {
      "console": true,
      "process": true,
      "require": true,
      "module": true
    },
    "unused": true,
    "asi": true,
    "loopfunc": true
  },
  "prettier": {
    "semi": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dinosConfig": {
    "servers": [
      "1.1.1.1",
      "8.8.8.8",
      "9.9.9.9",
      "8.8.4.4",
      "149.112.112.112",
      "1.0.0.1",
      "185.228.168.9",
      "185.228.169.9"
    ]
  }
}
